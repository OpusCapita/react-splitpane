    "sinon": "9.0.1",
    "style-loader": "1.1.3",
    "styled-components": "5.0.1",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2",
    "webpack-notifier": "1.8.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "patch-package": "6.2.1",
    "react-split-pane": "0.1.91"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/react-splitpane.git"
  }
}
